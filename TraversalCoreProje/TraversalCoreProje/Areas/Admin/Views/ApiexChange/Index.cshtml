@model IEnumerable<BookingExchangeViewModel.Exchange_Rates>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayaut.cshtml";
}


<h3>Api Ziyaretçi Listesi</h3>

<table class="table table-bordered">
    <tr>
        <th>Kur Adı</th>
        <th>Kur Oranı</th>
       

    </tr>

    @foreach (var item in Model)
    {
        <tr>
           
            <td>@item.currency</td>
            <td>@item.exchange_rate_buy</td>

        </tr>

    }

</table>
