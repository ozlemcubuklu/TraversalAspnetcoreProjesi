@model IEnumerable<ApiMovie>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayaut.cshtml";
}


<h3>Api Ziyaretçi Listesi</h3>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Resim</th>
        <th>Film Adı</th>
        <th>Imdb Puanı</th>
        <th>Açıklama</th>
        <th>Yapım Yılı</th>
     
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.rank</td>
            <td><img src="@item.image" width="50" height="50" /></td>
            <td>@item.title</td>
            <td>@item.rating</td>
            <td>@item.description</td>
            <td>@item.year</td>

        </tr>

    }

</table>
