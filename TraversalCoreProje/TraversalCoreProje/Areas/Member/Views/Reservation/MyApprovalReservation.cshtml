
@{
    ViewData["Title"] = "MyApprovalReservation";
    Layout = "~/Views/Shared/_MemberLayout.cshtml";
}
@model List<Reservation>
@if (Model.Count() > 0)
{



    <table class="table">
        <thead>
            <tr>
                <th>#</th>
                <th>Lokasyon</th>
                <th>Kişi Sayısı</th>
                <th>Rezervasyon Tarihi</th>
                <th>Durum</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Destination.City</td>
                    <td>@item.PersonCount</td>
                    <td>@item.ResevationDate</td>
                    <td>@item.Status</td>
                </tr>
            }
        </tbody>
    </table>

}else{
<div class="alert alert-danger" role="alert">
    Kayıt Bulunamadı..
</div>
}

