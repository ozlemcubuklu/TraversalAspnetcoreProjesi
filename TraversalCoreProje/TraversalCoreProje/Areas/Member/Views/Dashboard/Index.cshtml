                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--/ eCommerce statistic -->
                <!-- Statistics -->
                <div class="row match-height">


                    @await Component.InvokeAsync("_MemberStatistic")
                    <div class="col-xl-4 col-lg-12">
